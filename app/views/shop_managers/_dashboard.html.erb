<div id="page-content-wrapper">
    <h3>SHOP MANAGER DASHBOARD</h3>
    #TODO: Shop Manager Summary
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
            <%= link_to 'Add product' , new_shop_product_path(@shop), class: 'btn btn-primary' %>
            <% @products.each do |product|%>
              <%= product.product_name %>
              <%= product.shop_id %>
            <%end%>
            <%= yield :dashboard %>
            </div>
        </div>
    </div>
</div>
